<table class="table table-dark">
    <thead>
        <tr>
            <th scope="row">id</th>
            <th scope="row">title</th>
        </tr>
    </thead>
    <tbody>
        <% films.forEach(f => {%>
        <tr>
            <td scope="row"><%= f.film_id%></td>
            <td scope="row"><%= f.title%></td>
        </tr>
        <%}) %>
    </tbody>
</table>